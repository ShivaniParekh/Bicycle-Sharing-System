<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#4fb3bf</item>
        <item name="colorPrimaryDark">#005662</item>
        <item name="colorAccent">#4fb3bf</item>
    </style>

    <style name="Subscription" parent="Base.Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">#1faa00</item>
        <item name="colorAccent">#2196f3</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="PaymentTheme" parent="Base.Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/wallcolorPrimary</item>
        <item name="colorPrimaryDark">@color/wallcolorPrimaryDark</item>
        <item name="colorAccent">@color/wallcolorAccent</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

</resources>
